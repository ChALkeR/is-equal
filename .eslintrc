{
	"root": true,

	"extends": "@ljharb",

	"rules": {
		"complexity": [1, 10],
		"id-length": [2, { "min": 1, "max": 23 }],
		"max-statements": [1, 10],
		"no-extra-parens": [1],
		"no-implicit-coercion": [2, {
			"boolean": false,
			"number": false,
			"string": true
		}],
		"no-new-func": [1],
	}
}
